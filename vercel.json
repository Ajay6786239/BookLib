{
  "version": 2,
  "builds": [
    {
      "src": "myapp/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    },
    {
      "src": "staticfiles/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "BookLIb/static/BookLib(.*)",
      "dest": "staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myapp/wsgi.py"
    }
  ],
  {
  "scripts": {
    "build": "[framework build] --output public"
  }
},

